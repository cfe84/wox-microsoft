{
  "name": "wox-teams",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": [
        "ts",
        "json"
      ]
    }
  },
  "scripts": {
    "pack": "node pack.js",
    "bnp": "npm run build && npm run pack",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "clean": "rimraf dist/",
    "rebuild": "npm run clean && npm run build",
    "prepublish": "npm run rebuild",
    "start": "npm run build && node dist/index.js",
    "query": "ts-node src/index.ts \"`cat requests/${npm_config_request}.json`\"",
    "watch": "npm-watch"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^16.10.9",
    "adm-zip": "^0.5.9",
    "npm-watch": "^0.11.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.4"
  },
  "woxPack": {
    "target": "teams.wox",
    "pullNodeModules": true,
    "include": [
      "plugin.json",
      "run.bat",
      "images",
      "dist"
    ]
  },
  "dependencies": {
    "@azure/msal-node": "^1.3.2",
    "@microsoft/microsoft-graph-client": "^3.0.0",
    "dotenv": "^10.0.0",
    "isomorphic-fetch": "^3.0.0",
    "open": "^8.3.0"
  }
}